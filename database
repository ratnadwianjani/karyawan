/* Create Database and Table */
/*create database coba;*/

use coba;

CREATE TABLE `users` (
  `id` int(11) NOT NULL auto_increment,
  `name` varchar(100),
  `title` varchar(100),
  `salary` varchar(15),
  PRIMARY KEY  (`id`)
);

CREATE TABLE `departemen` (
  `id` int(11) NOT NULL auto_increment,
  `name` varchar(100),
  PRIMARY KEY  (`id`)
);

CREATE TABLE `company` (
  `id` int(11) NOT NULL auto_increment,
  `name` varchar(100),
  PRIMARY KEY  (`id`)
);